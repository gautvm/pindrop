{
    "commit_message": "$1: $2: $3+",
    "commit_message_arguments": [
        {
            "argument": 1,
            "case": "lower",
            "type_enums": [
                "fix",
                "feat",
                "chore",
                "refactor",
                "docs",
                "void",
                "deps",
                "ci",
                "style"
            ]
        }
    ],
    "custom_tasks": [
        {
            "name": "fmt",
            "execute": [
                "cargo fmt"
            ]
        }
    ],
    "hooks": [
        "fmt"
    ]
}